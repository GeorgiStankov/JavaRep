<?xml version="1.0" encoding="utf8"?>
 <!DOCTYPE hibernatemapping PUBLIC 
  "//Hibernate/Hibernate Mapping DTD//EN"
  "http://www.hibernate.org/dtd/hibernatemapping3.0.dtd"> 
 
 <hibernatemapping>
    <class name="Reservation" table="reservations">
       <id name="id" type="int" column="id">
          <generator class="native"/>
       </id>
       <property name="username" column="username" type="string"/>
       <property name="projectionId" column="projection_id" type="int"/>
       <property name="row" column="row" type="int"/>
       <property name="col" column="col" type="int"/>
    </class>
 </hibernatemapping> 

